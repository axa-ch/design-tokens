{
  "name": "@axa-ch/design-tokens",
  "version": "3.0.0-beta.2",
  "type": "module",
  "description": "AXA Design Tokens",
  "main": "./tokens.js",
  "module": "./tokens.mjs",
  "types": "./tokens.d.ts",
  "keywords": [
    "design tokens",
    "css",
    "scss",
    "js",
    "AXA",
    "styling"
  ],
  "files": [
    "build",
    "tokens",
    "tailwind.config.js",
    "tokens.js",
    "tokens.cjs",
    "tokens.d.ts",
    "tokens.json",
    "tokens.scss",
    "tokens.css",
    "tokens.min.css"
  ],
  "exports": {
    ".": {
      "types": "./tokens.d.ts",
      "import": "./tokens.js",
      "require": "./tokens.cjs"
    },
    "./build/": "./build/",
    "./tokens.json": "./tokens.json",
    "./tokens.css": "./tokens.css",
    "./tokens.min.css": "./tokens.min.css",
    "./tokens.scss": "./tokens.scss",
    "./tailwind.config": "./tailwind.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axa-ch/design-tokens"
  },
  "scripts": {
    "pretest": "npm run build",
    "lint": "prettier --ignore-path .prettierignore . --check && biome lint .",
    "test": "npm run lint && sass tests/mixins.spec.scss tests/mixins.spec.css && vitest run",
    "coverage": "vitest run --coverage",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "vite-node build.ts && prettier --write --ignore-path .prettierignore tokens.json build && vite build",
    "postbuild": "tsc",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start": "npm run build && npm run storybook"
  },
  "author": "AXA (https://axa.ch)",
  "maintainers": [
    {
      "name": "Gianluca Guarini",
      "email": "gianluca.guarini@axa.ch"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@axa-ch/easy-config": "^2.1.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@divriots/style-dictionary-to-figma": "^0.4.0",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "chaestli": "^3.1.0",
    "clsx": "^2.1.1",
    "color-blend": "^4.0.0",
    "color-util": "^2.2.3",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.47",
    "postcss-custom-media": "^11.0.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "rimraf": "^5.0.10",
    "sass": "^1.80.3",
    "storybook": "^8.3.6",
    "style-dictionary": "^4.1.4",
    "style-dictionary-utils": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-node": "^2.1.3",
    "vitest": "^2.1.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.24.0"
  }
}
