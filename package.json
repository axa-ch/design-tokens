{
  "name": "@axa-ch/design-tokens",
  "version": "1.0.0",
  "description": "AXA Design Tokens",
  "main": "./index.cjs.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "design tokens",
    "css",
    "scss",
    "js",
    "AXA",
    "styling"
  ],
  "files": [
    "build",
    "tokens",
    "tailwind.config.js",
    "index.js",
    "index.d.ts",
    "index.cjs.js",
    "tokens.json",
    "tokens.scss",
    "tokens.css"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs.js",
      "types": "./index.d.ts"
    },
    "./build/": "./build/",
    "./tokens.json": "./tokens.json",
    "./tokens.css": "./tokens.css",
    "./tokens.scss": "./tokens.scss",
    "./tailwind.config": "./tailwind.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axa-ch/design-tokens"
  },
  "scripts": {
    "pretest": "npm run build",
    "lint": "prettier . --check && eslint . --max-warnings=0",
    "test": "npm run lint && sass tests/mixins.spec.scss tests/mixins.spec.css && vitest",
    "coverage": "vitest run --coverage",
    "clean": "rimraf build",
    "build": "npm run clean && style-dictionary build && prettier --write tokens.json build && vite build",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start": "npm run build && npm run storybook"
  },
  "author": "AXA (https://axa.ch)",
  "maintainers": [
    {
      "name": "Gianluca Guarini",
      "email": "gianluca.guarini@axa.ch"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@axa-ch/easy-config": "^1.0.1",
    "@divriots/style-dictionary-to-figma": "^0.4.0",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "chaestli": "^3.1.0",
    "clsx": "^1.2.1",
    "color-blend": "^4.0.0",
    "color-util": "^2.2.3",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.25",
    "postcss-custom-media": "^10.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "storybook": "^7.0.27",
    "style-dictionary": "^3.8.0",
    "style-dictionary-utils": "^1.5.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0"
  }
}
